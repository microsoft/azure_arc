{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {      
        "resourceGroup": {
            "value": "resourceGroup-stage"
        },
        "namespace": {
            "value": "arc"
        },
        "connectionMode": {
            "value": "direct"
        },
        "controllerName": {
            "value": "jumpstart-dc"
        },
        "azdataUsername": {
            "value": "azdataUsername-stage"
        },
        "azdataPassword": {
            "value": "azdataPassword-stage"
        },
        "customLocation": {
            "value": "customLocation-stage"
        },
        "subscriptionId": {
            "value": "subscriptionId-stage"
        },        
        "spnClientId": {
            "value": "spnClientId-stage"
        },
        "spnTenantId": {
            "value": "spnTenantId-stage"
        },
        "spnClientSecret": {
            "value": "spnClientSecret-stage"
        },
        "spnAuthority": {
            "value": "https://login.microsoftonline.com"
        },
        "logAnalyticsWorkspaceId": {
            "value": "logAnalyticsWorkspaceId-stage"
        },
        "logAnalyticsPrimaryKey": {
            "value": "logAnalyticsPrimaryKey-stage"
        },
        "resourceTags": {
            "value": {
                "Project": "jumpstart_azure_arc_data_services"
            }
        },
        "dockerRegistryCredential": {
            "value": "arc-private-registry"
        },
        "dockerImagePullPolicy": {
            "value": "Always"
        },
