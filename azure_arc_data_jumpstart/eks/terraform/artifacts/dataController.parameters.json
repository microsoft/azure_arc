        "logsStorageClass": {
            "value": "gp2"
        },
        "logsStorageSize": {
            "value": "5Gi"
        },
        "serviceType": {
            "value": "LoadBalancer"
        },
        "controllerPort": {
            "value": 30080
        },
        "apiVersion": {
          "value": "2021-11-01"
        },
        "infrastructure": {
          "value": "aws"
        }
    }
