      "value": "microk8s-hostpath"
    },
    "logsStorageSize": {
      "value": "10Gi"
    },
    "serviceType": {
      "value": "LoadBalancer"
    },
    "controllerPort": {
      "value": 30080
    },
    "serviceProxyPort": {
      "value": 30777
    }
