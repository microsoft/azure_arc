{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "spnClientId": {
      "value": "[ClientId here - e.g. 338245b7-f268-4c07-812d-55b1d08f57e1]"
    },
    "spnClientSecret": {
      "value": "[Secret here - e.g. gug8Q~gl7woPYl5~-WnyrSD9f8jBzMWHpkk-ob51]"
    },
    "spnTenantId": {
      "value": "[Tenant Id here - e.g. 72f988bf-86f1-41af-91ab-2d7cd011db47"
    },
    "windowsAdminUsername": {
      "value": "arcdemo"
    },
    "windowsAdminPassword": {
      "value": "[Password here - e.g. ArcDemo123!!]"
    },
    "rdpPort": {
      "value": "13389"
    },
    "sshRSAPublicKey": {
      "value": "[RSA here - e.g. ssh-rsa  charris@CHARRIS01"
    },
    "deployBastion": {
      "value": false
    },
    "githubAccount": {
      "value": "microsoft"
    },
    "githubBranch": {
      "value": "jumpstart_ag"
    }
  }
}
