{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "envName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "spnClientId": {
      "value": "${SPN_CLIENT_ID}"
    },
    "spnClientSecret": {
      "value": "${SPN_CLIENT_SECRET}"
    },
    "spnTenantId": {
      "value": "${SPN_TENANT_ID}"
    },
    "windowsAdminUsername": {
      "value": "${JS_WINDOWS_ADMIN_USERNAME}"
    },
    "sshRSAPublicKey": {
      "value": "${JS_SSH_RSA_PUBLIC_KEY}"
    },
    "deployBastion": {
      "value": false
    },
    "rdpPort": {
      "value": "${JS_RDP_PORT}"
    },
    "githubUser": {
      "value": "${JS_GITHUB_USER}"
    },
    "githubPAT": {
      "value": "${JS_GITHUB_PAT}"
    }
  }
}