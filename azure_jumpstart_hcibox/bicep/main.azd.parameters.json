        "registerCluster": {
            "value": true
        },
        "deployResourceBridge": {
            "value": true
        },
        "deployAKSHCI": {
            "value": true
        },
        "githubAccount": {
            "value": "dkirby-ms"
        },
        "githubBranch": {
            "value": "azd_hcibox"
