{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "spnClientId": {
            "value": "<value for your service principal app id>"
        },
        "spnClientSecret": {
            "value": "<value for your service principal secret>"
        },
        "spnTenantId": {
            "value": "<value for your service principal tenant id>"
        },
        "windowsAdminUsername": {
            "value": "<value for your Windows admin username>"
        },
        "windowsAdminPassword": {
            "value": "<value for your Windows admin password>"
        },
        "logAnalyticsWorkspaceName": {
            "value": "<value for your Log Analytics workspace name>"
        },
        "deployBastion": {
            "value": false // true or false - set true to deploy Azure Bastion
        },
        "githubAccount": { 
            "value": "microsoft" // leave set to Microsoft unless working against a different Github account for development
        },
        "githubBranch": {
            "value": "jumpstart_ag" // leave set to jumpstart_ag unless working against a different branch name for development
        }
    }
}
